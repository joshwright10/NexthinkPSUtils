            <Questions>
              <Question Id="1" Label="Satisfaction">
                <Greeting>Dear {user}</Greeting>
                <Header>Windows 10 is around the corner. In order to better serve you during the transition phase,we would appreciate your feedback on the following questions.</Header>
                <Body>How happy are you with your current computer? </Body>
                <Answers Type="Scale" Orientation="Default">
                  <Answer Value="1" Id="0" Label="Not at all happy">
                    <Caption>Not at all happy</Caption>
                  </Answer>
                  <Answer Value="3" Id="3" Label="Slightly happy">
                    <Caption>Slightly happy</Caption>
                  </Answer>
                  <Answer Value="5" Id="1" Label="Moderately happy">
                    <Caption>Moderately happy</Caption>
                  </Answer>
                  <Answer Value="7" Id="2" Label="Very happy">
                    <Caption>Very happy</Caption>
                  </Answer>
                  <Answer Value="10" Id="4" Label="Completely happy">
                    <Caption>Completely happy</Caption>
                  </Answer>
                </Answers>
                <Footer />
                <Comment />
              </Question>
              <Question Id="2" Label="Positive points">
                <Body>What do you like about your computer?</Body>
                <Answers Type="MultipleAnswer" Orientation="Default">
                  <Answer Value="2" Id="1" Label="Hardware resources">
                    <Caption>It's fast</Caption>
                  </Answer>
                  <Answer Value="4" Id="3" Label="Device stability">
                    <Caption>It doesn't crash</Caption>
                  </Answer>
                  <Answer Value="6" Id="5" Label="Startup">
                    <Caption>It starts quickly</Caption>
                  </Answer>
                  <Answer Value="1" Id="0" Label="Display">
                    <Caption>The screen is large enough</Caption>
                  </Answer>
                  <Answer Value="3" Id="2" Label="Network">
                    <Caption>Internet is fast</Caption>
                  </Answer>
                  <Answer Value="5" Id="4" Label="Software">
                    <Caption>The applications are fast</Caption>
                  </Answer>
                  <Answer Value="7" Id="6" Label="Storage">
                    <Caption>There is enough storage</Caption>
                  </Answer>
                  <Answer Value="8" Id="7" Label="Complexity">
                    <Caption>It is easy to use</Caption>
                  </Answer>
                  <Answer Value="10" Id="9" Label="Battery">
                    <Caption>It has a long battery life (if applicable)</Caption>
                  </Answer>
                  <Answer Value="9" Id="8" Label="Nothing">
                    <Caption>There is nothing I like</Caption>
                  </Answer>
                </Answers>
                <Comment />
              </Question>
              <Question Id="3" Label="Negative points">
                <Body>What do you not like about your computer?</Body>
                <Answers Type="MultipleAnswer" Orientation="Default">
                  <Answer Value="2" Id="1" Label="Hardware resources">
                    <Caption>It's slow</Caption>
                  </Answer>
                  <Answer Value="4" Id="3" Label="Device stability">
                    <Caption>It crashes often</Caption>
                  </Answer>
                  <Answer Value="6" Id="5" Label="Startup">
                    <Caption>It takes too long to start up</Caption>
                  </Answer>
                  <Answer Value="1" Id="0" Label="Display">
                    <Caption>The screen is too small</Caption>
                  </Answer>
                  <Answer Value="3" Id="2" Label="Network">
                    <Caption>Internet is slow</Caption>
                  </Answer>
                  <Answer Value="5" Id="4" Label="Software">
                    <Caption>The applications are slow</Caption>
                  </Answer>
                  <Answer Value="7" Id="6" Label="Storage">
                    <Caption>There is not enough storage</Caption>
                  </Answer>
                  <Answer Value="8" Id="7" Label="Complexity">
                    <Caption>It is complicated to use</Caption>
                  </Answer>
                  <Answer Value="10" Id="9" Label="Battery">
                    <Caption>It has a short battery life (if applicable)</Caption>
                  </Answer>
                  <Answer Value="9" Id="8" Label="Nothing">
                    <Caption>There is nothing I don't like</Caption>
                  </Answer>
                </Answers>
                <Comment />
              </Question>
            </Questions>
